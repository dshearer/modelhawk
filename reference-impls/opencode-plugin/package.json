  "description": "modelhawk",
  "author": {
    "name": "C. Dylan Shearer",
    "email": "dylan@nekonya.info"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
